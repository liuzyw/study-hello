<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.spring.mapper.StudentSelfCardDao">

  <resultMap id="studentSelfCardMap" type="com.study.spring.po.StudentSelfCard">
    <id column="id" property="id"/>
    <result column="student_id" property="studentId"/>
    <result column="note" property="note"/>
    <result column="birthday" property="birthday"/>
    <result column="adress" property="adress"/>
  </resultMap>

  <select id="findStudentSelfCardByStudentId" parameterType="java.lang.Integer" resultMap="studentSelfCardMap">
    SELECT id, student_id, adress, birthday, note FROM t_student_self_card WHERE student_id = #{id}
  </select>


</mapper>